{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["config", "controllers", "db", "middleware", "routes", "validators"],
        "exclude": ["node_modules", "coverage"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}